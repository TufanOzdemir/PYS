@model ZabitaWebApplication.Models.Project

@{
    ViewBag.Title = "ProjectAddEmployee";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}


@using (Html.BeginForm())
{
    <div class="panel panel-info">
            <div class="col-md-6 col-md-offset-3 text-center">
                <hr />
                <div class="panel panel-title panel-heading panel-info">@Model.Name , Projesine Eleman Ekleyin </div>
                <hr />
                <div class="form-group">
                    <label for="email">Email Adresi</label>
                    <input name="email" type="email" class="form-control"  placeholder="Email"> 
                </div>
                <hr />
                
            </div>
        <div class="col-md-6 col-md-offset-3 text-center">
            <input type="submit" value="Kaydet" class="w3-button w3-green" />
        </div>
        </div>
}

